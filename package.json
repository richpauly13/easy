{
  "name": "easy-docs",
  "version": "1.0.0",
  "author": "Paul Chehak <pauly13@sbcglobal.net>",
  "description": "Documentation for the EASY framework.",
  "keywords": [
    "angular",
    "css",
    "flexbox",
    "responsive",
    "accessible",
    "framework"
  ],
  "bugs": {
    "url": "https://github.com/richpauly13/easy/issues"
  },
  "homepage": "https://richpauly13.github.io/easy/dist/easy-docs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "node node_modules/markdown-styleguide-generator/index.js",
    "watch-easy": "ng build easy --watch",
    "build": "npm run build-easy && npm run build-docs",
    "build-docs": "npm run docs && ng build easy-docs --prod",
    "build-easy": "ng build easy --prod && npm run npm-pack",
    "npm-pack": "cd dist/easy && npm pack",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "codecov": "node_modules/.bin/codecov --disable=gcov"
  },
  "dependencies": {
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~6.2.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.9.4",
    "codecov": "^3.1.0",
    "codelyzer": "~4.4.4",
    "gulp": "^4.0.0",
    "gulp-shell": "^0.6.5",
    "gulp-watch": "^5.0.1",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "markdown-styleguide-generator": "^2.0.4",
    "ng-packagr": "^4.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2"
  }
}
