  "dependencies": {
    "@angular/animations": "^8.2.0-rc.0",
    "@angular/common": "^8.2.0-rc.0",
    "@angular/compiler": "^8.2.0-rc.0",
    "@angular/core": "^8.2.0-rc.0",
    "@angular/forms": "^8.2.0-rc.0",
    "@angular/platform-browser": "^8.2.0-rc.0",
    "@angular/platform-browser-dynamic": "^8.2.0-rc.0",
    "@angular/router": "^8.2.0-rc.0",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.0"
  },
  "description": "Documentation for EASY framework.",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.0-next.1",
    "@angular-devkit/build-ng-packagr": "^0.802.0-next.1",
    "@angular-devkit/core": "^8.2.0-next.1",
    "@angular-devkit/schematics": "^8.2.0-next.1",
    "@angular/cli": "^8.2.0-next.1",
    "@angular/compiler-cli": "^8.2.0-rc.0",
    "@angular/language-service": "^8.2.0-rc.0",
    "@schematics/angular": "^8.2.0-next.1",
    "@types/jasmine": "^3.3.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.6.8",
    "codecov": "^4.0.0-0",
    "codelyzer": "^5.1.0",
    "cpx": "^1.5.0",
    "husky": "^3.0.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.0",
    "standard-version": "^6.0.1",
    "ts-node": "^8.3.0",
    "tsickle": "^0.36.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5"
  },
  "homepage": "https://github.com/richpauly13/easy#readme",
  "keywords": [
    "a11y",
    "accessible",
    "angular",
    "css",
    "flexbox",
    "framework",
    "responsive"
  ],
  "license": "MIT",
  "name": "easy",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "scripts": {
    "build": "run-s build:easy build:docs",
    "build:docs": "ng build easy-docs --prod",
    "build:easy": "ng build easy-framework",
    "build:schematics": "tsc -p projects/easy-framework/tsconfig.schematics.json",
    "copy:all": "run-p copy:changes copy:md copy:schematics",
    "copy:changes": "cpx projects/easy-framework/CHANGELOG.md ./",
    "copy:md": "cpx {LICENSE.md,README.md} dist/easy-framework",
    "copy:schematics": "cpx projects/easy-framework/schematics/**/{collection.json,files/**,schema.json} dist/easy-framework/schematics",
    "e2e": "npm run update:webdriver && ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "pack": "cd dist/easy-framework && npm pack",
    "package": "run-s build:easy build:schematics copy:all pack",
    "release": "cd projects/easy-framework && standard-version --no-verify && cd ../.. && npm run package && git push --follow-tags origin master && npm publish ./dist/easy-framework --access public",
    "start": "ng serve -o",
    "test": "ng test",
    "test:docs": "ng test easy-docs --browsers=ChromeHeadlessCI",
    "test:easy": "ng test easy-framework --browsers=ChromeHeadlessCI",
    "update:ng": "ng update --all --next --force --allowDirty && npm i --save-dev typescript@3.4.5",
    "update:webdriver": "webdriver-manager update",
    "watch:easy": "ng build easy-framework --watch"
  },
  "version": "1.0.0"
}
