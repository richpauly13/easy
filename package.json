{
  "name": "easy-docs",
  "version": "0.0.1",
  "author": "Paul Chehak <pauly13@sbcglobal.net>",
  "description": "Documentation for EASY framework.",
  "keywords": [
    "angular",
    "css",
    "flexbox",
    "responsive",
    "accessible",
    "framework",
    "a11y"
  ],
  "bugs": {
    "url": "https://github.com/richpauly13/easy/issues"
  },
  "homepage": "https://github.com/richpauly13/easy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "private": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "test": "ng test",
    "test:easy": "ng test easy --browsers=ChromeHeadlessCI",
    "test:docs": "ng test easy-docs --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "npm run update:webdriver && ng e2e --no-webdriver-update",
    "docs": "node node_modules/markdown-styleguide-generator/index.js",
    "watch:easy": "ng build easy --watch",
    "build": "run-s build:easy build:docs",
    "build:easy": "ng build easy --prod",
    "build:docs": "npm run docs && ng build easy-docs --prod",
    "format:fix": "pretty-quick --staged",
    "copy:all": "run-p copy:license copy:readme",
    "copy:license": "copy .\\LICENSE.md .\\dist\\easy",
    "copy:readme": "copy .\\README.md .\\dist\\easy",
    "pack": "cd dist/easy && npm pack",
    "package": "run-s build:easy copy:all pack",
    "publish:easy": "cd dist/easy && npm publish easy-framework-0.0.19.tgz",
    "update:ng": "ng update --all --force && npm i --save-dev typescript@3.2.4",
    "update:webdriver": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.46"
  },
  "dependencies": {
    "@angular/common": "^7.2.12",
    "@angular/compiler": "^7.2.12",
    "@angular/core": "^7.2.12",
    "@angular/forms": "^7.2.12",
    "@angular/http": "^7.2.12",
    "@angular/platform-browser": "^7.2.12",
    "@angular/platform-browser-dynamic": "^7.2.12",
    "@angular/router": "^7.2.12",
    "core-js": "^3.0.1",
    "rxjs": "^6.4.0",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.12",
    "@angular/language-service": "^7.2.12",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.13.0",
    "child_process": "^1.0.2",
    "codecov": "^4.0.0-0",
    "codelyzer": "^5.0.0",
    "gulp": "^4.0.0",
    "husky": "^1.3.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "^0.0.32",
    "markdown-styleguide-generator": "^2.0.4",
    "ng-packagr": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "^6.0.0",
    "ts-node": "^8.0.3",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.3",
    "tslint": "^5.15.0",
    "typescript": "^3.2.4"
  }
}
