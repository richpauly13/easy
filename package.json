  "name": "easy",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "scripts": {
    "build": "run-s build:easy build:docs",
    "build:docs": "ng build easy-docs --prod",
    "build:easy": "ng build easy-framework --prod",
    "bump:deps": "npm run build && git add . && git commit -m \"chore(deps): bump deps\" && git push",
    "e2e": "npm run update:webdriver && ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve -o",
    "test": "ng test",
    "test:docs": "ng test easy-docs",
    "test:easy": "ng test easy-framework",
    "test:watch": "ng test easy-framework --watch",
    "update:core": "npm i -g npm && npm i -g @angular/cli@next",
    "update:deps": "ng update --next --all --force --allowDirty && npm i -D typescript@3.9.7 && npm i --save rxjs@6.6.2",
    "update:ng": "run-s update:core update:deps",
    "update:webdriver": "webdriver-manager update"
  },
  "version": "1.0.0"
}
