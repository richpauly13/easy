  },
  "scripts": {
    "build": "run-s build:easy build:docs",
    "build:docs": "ng build easy-docs",
    "build:easy": "ng build easy-framework",
    "bump:deps": "npm run build && git add . && git commit -m \"chore(deps): bump deps\" && git push",
    "deploy": "ng deploy",
    "lint": "ng lint --fix",
    "ng": "ng",
    "release": "cd projects/easy-framework && npm run release",
    "start": "ng serve -o",
    "test": "ng build && ng test",
    "test:add": "cd projects/easy-framework && npm run test",
    "test:docs": "ng test easy-docs",
    "test:easy": "ng test easy-framework",
    "test:watch": "ng test --watch",
    "update:core": "ng update @angular/cli @angular/core --allow-dirty",
    "update:deps": "npm update && npm i",
    "update:global": "npm i -g npm@7 @angular/cli@latest",
    "update:ng": "run-s update:global update:core update:deps",
    "watch:easy": "ng build easy-framework --watch"
  },
  "version": "1.0.0"
}
