  "scripts": {
    "add": "git add .",
    "build": "ng build easy-framework && ng build easy-docs",
    "bump-deps": "run-s add commit push",
    "change": "cd projects/easy-framework",
    "commit": "git commit -m \"chore(deps): bump deps\"",
    "deploy": "ng deploy",
    "lint": "ng lint --fix",
    "ng": "ng",
    "push": "git push",
    "release": "run-s change release",
    "start": "ng serve -o",
    "test": "run-s build test-all",
    "test-add": "run-s change test",
    "test-all": "ng test",
    "test-watch": "ng test --watch",
    "update-core": "ng update @angular/core @angular/cli @angular-eslint/schematics --allow-dirty --force",
    "update-deps": "npm update --force && npm i --force",
    "update-global": "npm i -g npm@8 @angular/cli@latest",
    "update-ng": "run-s update-global update-core update-deps",
    "watch-easy": "ng build --watch"
  },
