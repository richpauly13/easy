{
  "name": "easy-docs",
  "version": "0.0.1",
  "author": "Paul Chehak <pauly13@sbcglobal.net>",
  "description": "Documentation for the EASY framework.",
  "keywords": [
    "angular",
    "css",
    "flexbox",
    "responsive",
    "accessible",
    "framework"
  ],
  "bugs": {
    "url": "https://github.com/richpauly13/easy/issues"
  },
  "homepage": "https://richpauly13.github.io/easy/dist/easy-docs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "test": "ng test",
    "test:easy": "ng test easy --browsers=ChromeHeadlessCI",
    "test:docs": "ng test easy-docs --browsers=ChromeHeadlessCI",
    "lint": "npm run format && ng lint",
    "e2e": "ng e2e",
    "docs": "node node_modules/markdown-styleguide-generator/index.js",
    "watch:easy": "ng build easy --watch",
    "build": "npm run build:easy && npm run build:docs",
    "build:docs": "npm run docs && ng build easy-docs --prod",
    "build:easy": "ng build easy --prod && npm run pack",
    "pack": "cd dist/easy && npm pack",
    "format": "pretty-quick",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "precommit": "npm run lint",
    "publish": "npm run build:easy && npm publish dist/easy"
  },
  "dependencies": {
    "@angular/common": "^7.1.3",
    "@angular/compiler": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/router": "^7.1.3",
    "core-js": "^2.6.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.3",
    "@angular-devkit/build-ng-packagr": "^0.11.3",
    "@angular/cli": "^7.1.3",
    "@angular/compiler-cli": "^7.1.3",
    "@angular/language-service": "^7.1.3",
    "@types/jasmine": "^3.3.2",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.15",
    "codecov": "^3.1.0",
    "codelyzer": "^4.5.0",
    "gulp": "^4.0.0",
    "gulp-shell": "^0.6.5",
    "gulp-watch": "^5.0.1",
    "husky": "^1.2.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "0.0.32",
    "markdown-styleguide-generator": "^2.0.4",
    "ng-packagr": "^4.4.5",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
