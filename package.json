{
  "name": "easy-docs",
  "version": "0.0.1",
  "author": "Paul Chehak <pauly13@sbcglobal.net>",
  "description": "Documentation for the EASY framework.",
  "keywords": [
    "angular",
    "css",
    "flexbox",
    "responsive",
    "accessible",
    "framework",
    "a11y"
  ],
  "bugs": {
    "url": "https://github.com/richpauly13/easy/issues"
  },
  "homepage": "https://github.com/richpauly13/easy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "private": true,
  "engines": {
    "node": "10.13.0",
    "npm": "6.5.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "test": "ng test",
    "test:easy": "ng test easy --browsers=ChromeHeadlessCI",
    "test:docs": "ng test easy-docs --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "node node_modules/markdown-styleguide-generator/index.js",
    "watch:easy": "ng build easy --watch",
    "build": "run-s build:easy build:docs",
    "build:easy": "ng build easy --prod",
    "build:docs": "npm run docs && ng build easy-docs --prod",
    "pack": "cd dist/easy && npm pack",
    "format:fix": "pretty-quick --staged",
    "update:webdriver": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "publish": "npm run build:easy && npm run pack && cd dist/easy && npm publish",
    "update:ng": "ng update --all"
  },
  "dependencies": {
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/router": "^7.2.0",
    "core-js": "^2.6.2",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular-devkit/build-ng-packagr": "^0.12.1",
    "@angular/cli": "^7.2.2",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "child_process": "^1.0.2",
    "codecov": "^3.1.0",
    "codelyzer": "^4.5.0",
    "gulp": "^4.0.0",
    "husky": "^1.3.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "^0.0.32",
    "markdown-styleguide-generator": "^2.0.4",
    "ng-packagr": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "protractor": "^5.4.2",
    "ts-node": "^7.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  }
}
