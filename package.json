{
  "name": "easy",
  "version": "1.0.0",
  "author": "Paul Chehak <pauly13@att.net>",
  "bugs": {
    "url": "https://github.com/richpauly13/easy/issues"
  },
  "description": "Documentation for EASY framework.",
  "homepage": "https://github.com/richpauly13/easy#readme",
  "keywords": [
    "a11y",
    "accessible",
    "angular",
    "css",
    "flexbox",
    "framework",
    "grid",
    "responsive",
    "sass"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "scripts": {
    "build": "run-s build:easy build:docs",
    "build:docs": "ng build easy-docs --prod",
    "build:easy": "ng build easy-framework --prod",
    "bump:deps": "npm run build && git add . && git commit -m \"chore(deps): bump deps\" && git push",
    "copy:md": "copyfiles {LICENSE.md,README.md} ./dist/easy-framework",
    "e2e": "npm run update:webdriver && ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "package": "run-s build:easy copy:md",
    "release": "npm run version && git push --follow-tags origin master && npm run package && npm publish ./dist/easy-framework --access public",
    "start": "ng serve -o",
    "test": "ng test",
    "test:docs": "ng test easy-docs",
    "test:easy": "ng test easy-framework",
    "test:watch": "ng test --watch",
    "update:cli": "ng update @angular/cli --next --force --allow-dirty",
    "update:core": "ng update @angular/core --next --force --allow-dirty",
    "update:deps": "npm update && npm i",
    "update:global": "npm i -g npm@latest && npm i -g @angular/cli@next",
    "update:ng": "run-s update:global update:cli update:core update:deps",
    "update:webdriver": "webdriver-manager update",
    "version": "cd projects/easy-framework && standard-version",
    "watch:easy": "ng build easy-framework --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.0-next.2",
    "@angular/common": "^11.1.0-next.2",
    "@angular/compiler": "^11.1.0-next.2",
    "@angular/core": "^11.1.0-next.2",
    "@angular/forms": "^11.1.0-next.2",
    "@angular/platform-browser": "^11.1.0-next.2",
    "@angular/platform-browser-dynamic": "^11.1.0-next.2",
    "@angular/router": "^11.1.0-next.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1101.0-next.2",
    "@angular-devkit/build-angular": "^0.1101.0-next.2",
    "@angular-eslint/builder": "^0.8.0-beta.5",
    "@angular-eslint/eslint-plugin": "^0.8.0-beta.5",
    "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.5",
    "@angular-eslint/schematics": "^0.8.0-beta.5",
    "@angular-eslint/template-parser": "^0.8.0-beta.5",
    "@angular/cli": "^11.1.0-next.2",
    "@angular/compiler-cli": "^11.1.0-next.2",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.12",
    "@typescript-eslint/eslint-plugin": "^4.9.2-alpha.0",
    "@typescript-eslint/parser": "^4.9.2-alpha.1",
    "codecov": "^4.0.0-0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "husky": "^4.3.5",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.3",
    "npm-run-all": "^4.1.5",
    "protractor": "^7.0.0",
    "standard-version": "^9.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  }
}
