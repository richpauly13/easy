{
  "name": "easy",
  "version": "1.0.0",
  "author": "Paul Chehak <pauly13@sbcglobal.net>",
  "description": "Documentation for EASY framework.",
  "keywords": [
    "angular",
    "css",
    "flexbox",
    "responsive",
    "accessible",
    "framework",
    "a11y"
  ],
  "bugs": {
    "url": "https://github.com/richpauly13/easy/issues"
  },
  "homepage": "https://github.com/richpauly13/easy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/richpauly13/easy.git"
  },
  "scripts": {
    "build:docs": "ng build easy-docs --prod",
    "build:easy": "ng build easy-framework",
    "build": "run-s build:easy build:docs",
    "copy:all": "run-p copy:license copy:readme",
    "copy:license": "copy .\\LICENSE.md .\\dist\\easy-framework",
    "copy:readme": "copy .\\README.md .\\dist\\easy-framework",
    "e2e": "npm run update:webdriver && ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "pack": "cd dist/easy-framework && npm pack",
    "package": "run-s build:easy copy:all pack",
    "publish:easy": "cd dist/easy-framework && npm publish easy-framework-0.2.0.tgz",
    "start": "ng serve -o",
    "test:docs": "ng test easy-docs --browsers=ChromeHeadlessCI",
    "test:easy": "ng test easy-framework --browsers=ChromeHeadlessCI",
    "test": "ng test",
    "update:ng": "ng update --all --next --force --allowDirty && npm i --save-dev typescript@3.4.5",
    "update:webdriver": "webdriver-manager update",
    "watch:easy": "ng build easy-framework --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.2.0-next.1",
    "@angular/compiler": "^8.2.0-next.1",
    "@angular/core": "^8.2.0-next.1",
    "@angular/forms": "^8.2.0-next.1",
    "@angular/platform-browser": "^8.2.0-next.1",
    "@angular/platform-browser-dynamic": "^8.2.0-next.1",
    "@angular/router": "^8.2.0-next.1",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.0-next.0",
    "@angular-devkit/build-ng-packagr": "^0.802.0-next.0",
    "@angular/cli": "^8.2.0-next.1",
    "@angular/compiler-cli": "^8.2.0-next.1",
    "@angular/language-service": "^8.2.0-next.1",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.6.2",
    "codecov": "^4.0.0-0",
    "codelyzer": "^5.1.0",
    "husky": "^3.0.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.0",
    "ts-node": "^8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5"
  }
}
