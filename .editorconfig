# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
insert_final_newline = true
tab_width = 4
trim_trailing_whitespace = true

[*.ts]
quote_type = single

[*.json]
indent_size = 2
indent_style = space

[*.md]
max_line_length = off
trim_trailing_whitespace = false
